{
    "name": "jobs",
    "version": "1.0.0",
    "description": "Offline jobs",
    "main": "dist/server.js",
    "scripts": {
        "start": "yarn run serve",
        "build": "yarn run clean && tsc --build",
        "build:lint": "yarn run clean && yarn run lint && tsc --build",
        "build:run": "yarn run build && yarn run start",
        "serve": "cp .env ../. && node dist/server.js",
        "watch": "concurrently -k \"tsc --build --watch\" \"nodemon ./dist/server.js\"",
        "clean": "rm -rf dist/",
        "lint": "tsc --noEmit && eslint \"*/**/*.ts\" --quiet --fix",
        "prod": "npm run build && npm prune --production"
    },
    "dependencies": {
        "@bugsnag/js": "7.6.0",
        "@negobot/shared": "1.0.0",
        "@sendgrid/mail": "7.4.2",
        "bcryptjs": "2.4.3",
        "dotenv": "8.2.0",
        "inversify": "5.0.5",
        "jsonwebtoken": "8.5.1",
        "nanoid": "3.1.22",
        "pg": "8.5.1",
        "reflect-metadata": "0.1.13",
        "restify-cors-middleware": "1.1.1",
        "typeorm": "0.2.35",
        "uuid": "8.3.2",
        "yup": "0.29.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "2.4.2",
        "@types/jest": "26.0.20",
        "@types/jsonwebtoken": "8.5.0",
        "@types/node": "14.14.22",
        "@types/passport-jwt": "3.0.4",
        "@types/pg": "^8.6.0",
        "@types/restify": "8.5.0",
        "@types/restify-cors-middleware": "1.0.1",
        "@types/restify-errors": "4.3.3",
        "@types/uuid": "8.3.0",
        "@types/yup": "0.29.3",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2",
        "@types/node-fetch": "2.5.12",
        "concurrently": "6.1.0",
        "eslint": "7.19.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-prettier": "3.3.1",
        "inversify": "5.0.5",
        "inversify-restify-utils": "3.4.1",
        "csv-parser": "3.0.0",
        "neat-csv": "6.0.1",
        "twilio": "3.61.0",
        "nodemon": "2.0.7",
        "prettier": "2.2.1",
        "tsc-watch": "4.2.9",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    },
    "author": "Goavega Software",
    "license": "ISC"
}